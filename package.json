{
  "name": "wechaty-room-message-forward",
  "version": "1.0.0",
  "description": "Wechaty conversational SDK getting start template & examples",
  "type": "module",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_OPTIONS='--no-warnings --loader=ts-node/esm' node src/main.ts",
    "start:js": "node dist/main.js",
    "debug": "cross-env WECHATY_LOG=silly npm start",
    "debug:log": "cross-env WECHATY_LOG=verbose WECHATY_LOG_PREFIX=\"(StarterBot|App)\" npm start"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "dependencies": {
    "qrcode-terminal": "^0.12.0",
    "watchdog": "^0.8.17",
    "wechaty": "^1.18.1",
    "wechaty-puppet-wechat": "^1.18.4"
  },
  "devDependencies": {
    "@chatie/eslint-config": "^1.0.4",
    "@chatie/tsconfig": "^4.6.3"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  }
}
